{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "max-len": [2, {"code": 140, "tabWidth": 2, "ignoreUrls": true}],
    "react/prefer-stateless-function": "off",
    "no-plusplus": "off"
  }
}